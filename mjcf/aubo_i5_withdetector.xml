<mujoco model="aubo_i5">
  <compiler angle="radian" autolimits="true"/>

  <default>
    <joint stiffness="0" damping="0.1" armature="0.1"/>
    <default class="visual">
      <geom type="mesh" contype="0" conaffinity="0" group="1" density="0"/>
    </default>
    <default class="collision">
      <geom contype="1" conaffinity="1" group="3" density="1000"/>
    </default>
    <default class="arm_motor">
      <motor gear="1" ctrllimited="true" forcelimited="true" ctrlrange="-5000 5000" forcerange="-80 80"/>
    </default>
    <default class="arm_position">
      <joint range="-3.04 3.04" limited="true"/>
      <position  kp="4000" kv="300" ctrlrange="-3.04 3.04" forcerange="-80 80"/>
    </default>
    <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4"/>
  </default>

  <asset>
    <material class="visual" name="orange" rgba="0.996 0.369 0.063 1" specular="0.5" shininess="0.5"/>
    <material class="visual" name="black" rgba="0 0 0 1" specular="0.8" shininess="0.8"/>

    <material name="metallic_black" rgba="0.15 0.15 0.15 1" specular="0.9" shininess="0.9"/>

    <mesh name="base_link" file="../models/base_link.STL"/>
    <mesh name="shoulder_Link" file="../models/shoulder_Link.STL"/>
    <mesh name="upperArm_Link" file="../models/upperArm_Link.STL"/>
    <mesh name="foreArm_Link" file="../models/foreArm_Link.STL"/>
    <mesh name="wrist1_Link" file="../models/wrist1_Link.STL"/>
    <mesh name="wrist2_Link" file="../models/wrist2_Link.STL"/>
    <mesh name="wrist3_Link" file="../models/wrist3_Link.STL"/>
    <mesh name="connector_mesh" file="../models/connector_PHOB.stl"/>
    <mesh name="detector_mesh" file="../models/detector.stl"/>
  </asset>

  <option timestep="0.001" gravity="0 0 -9.81" integrator="implicitfast" density="1.0" viscosity="1.8e-5"/>

  <worldbody>
    <geom type="mesh" material="black" class="visual" mesh="base_link"/>
    <body name="shoulder_Link" pos="0 0 0.122" quat="-3.67321e-06 0 0 1">
      <inertial pos="3.25089e-07 0.00534955 -0.00883689" quat="0.99587 -0.0907855 0 0" mass="1.57658" diaginertia="0.00406404 0.00395808 0.00305754"/>
      <joint name="shoulder_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.04 3.04" limited="true"/>
      <geom type="mesh" mesh="shoulder_Link" class="visual" material="orange"/>
      <geom name="shoulder_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="mesh" mesh="shoulder_Link" class="collision" />
      <body name="upperArm_Link" pos="0 0.1215 0" quat="0.499998 -0.5 -0.5 -0.500002">
        <inertial pos="0.203997 2.01305e-10 0.0127642" quat="0.5 0.5 0.5 0.5" mass="4.04176" diaginertia="0.144994 0.142607 0.00965399"/>
        <joint name="upperArm_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.04 3.04" limited="true"/>
        <geom type="mesh" mesh="upperArm_Link" class="visual" material="orange"/>
        <geom name="upperArm_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="mesh" mesh="upperArm_Link" class="collision" />
        <body name="foreArm_Link" pos="0.408 0 0" quat="-3.67321e-06 -1 0 0">
          <inertial pos="0.188922 6.78882e-07 0.0981027" quat="0.504333 0.495629 0.495629 0.504333" mass="2.27146" diaginertia="0.0443926 0.0441401 0.00213049"/>
          <joint name="foreArm_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.04 3.04" limited="true"/>
          <geom type="mesh" mesh="foreArm_Link" class="visual" material="orange"/>
          <geom name="foreArm_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="mesh" mesh="foreArm_Link" class="collision" />
          <body name="wrist1_Link" pos="0.376 0 0" quat="-2.59734e-06 0.707105 0.707108 -2.59735e-06">
            <inertial pos="7.54205e-07 0.00624813 -0.00392367" quat="0.677608 0.735423 0 0" mass="0.500478" diaginertia="0.000711946 0.000687466 0.000403991"/>
            <joint name="wrist1_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.04 3.04" limited="true"/>
            <geom type="mesh" mesh="wrist1_Link" class="visual" material="orange"/>
            <geom name="wrist1_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="mesh" mesh="wrist1_Link" class="collision" />
            <body name="wrist2_Link" pos="0 0.1025 0" quat="0.707105 -0.707108 0 0">
              <inertial pos="-7.54208e-07 -0.00624813 -0.00392367" quat="0.735423 0.677608 0 0" mass="0.500478" diaginertia="0.000711946 0.000687466 0.000403991"/>
              <joint name="wrist2_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.04 3.04" limited="true"/>
              <geom type="mesh" mesh="wrist2_Link" class="visual" material="orange"/>
              <geom name="wrist2_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="mesh" mesh="wrist2_Link" class="collision" />
              <body name="wrist3_Link" pos="0 -0.094 0" quat="0.707105 0.707108 0 0">
                <inertial pos="3.92049e-10 0.000175788 -0.0213294" quat="0.5 0.5 -0.5 0.5" mass="0.15831" diaginertia="0.000108772 7.31376e-05 7.19528e-05"/>
                <joint name="wrist3_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.04 3.04" limited="true"/>
                <geom type="mesh" mesh="wrist3_Link" class="visual" material="orange"/>
                <geom name="wrist3_collision" pos="0 0 0" quat="1.0 0.0 0.0 0.0" type="mesh" mesh="wrist3_Link" class="collision" />
                <site name="ee_site" pos="0 0.1 0" quat="-1 1 0 0"/>
                <body name="connector_body" pos="0.0631 0.0322 0.0365" euler="0 0 0">
                      <geom type="mesh" mesh="connector_mesh" material="metallic_black" class="visual"/>
                      <geom type="mesh" mesh="connector_mesh" class="collision"/>
                      <body name="detector_body" pos="-0.1031 0.008 0.06" euler="0 0 0">
                          <geom type="mesh" mesh="detector_mesh" material="metallic_black" class="visual"/>
                          <geom type="mesh" mesh="detector_mesh" class="collision"/>
                          <camera name="ee_camera" pos="0.04 0 0" euler="3.14 0 0" fovy="60"/>
                      </body>
                  </body>

              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <position name="shoulder_joint" joint="shoulder_joint" class="arm_position" ctrlrange="-3.04 3.04"/>
    <position name="upperArm_joint" joint="upperArm_joint" class="arm_position" ctrlrange="-3.04 3.04"/>
    <position name="foreArm_joint"  joint="foreArm_joint"  class="arm_position" ctrlrange="-3.04 3.04"/>
    <position name="wrist1_joint"   joint="wrist1_joint"   class="arm_position" ctrlrange="-3.04 3.04"/>
    <position name="wrist2_joint"   joint="wrist2_joint"   class="arm_position" ctrlrange="-3.04 3.04"/>
    <position name="wrist3_joint"   joint="wrist3_joint"   class="arm_position" ctrlrange="-3.04 3.04"/>
  </actuator>

</mujoco>
